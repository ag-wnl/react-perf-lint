    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "license": "ISC"
}
